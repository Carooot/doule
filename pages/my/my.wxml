<view class="container">
  <view class="userinfo">
    <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
    <text class="userinfo-nickname">{{userInfo.nickName}}</text>
  </view>
  <view class="list">
  	<view wx:for="{{mylist}}">
  		<view class="list-item">
  			<image src="{{item.url}}"></image>
  			<text>{{item.name}}</text>
  		</view>
  	</view>
  </view>
</view>